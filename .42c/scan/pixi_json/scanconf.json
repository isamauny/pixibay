                }
            },
            "/picture/upload": {
                "post": {
                    "happyPaths": [
                        {
                            "key": "happy.path.success",
                            "requests": [
                                {
                                    "environment": {
                                        "contents": "",
                                        "title": "upload from create pic"
                                    },
                                    "$ref": "#/requests/uploadPicture"
                                }
                            ]
                        }
                    ]
                }
            },
            "/picture/{id}": {
                "delete": {
                    "authorizationTests": [
                        "DeletePicture"
                    ],
                    "happyPaths": [
                        {
                            "key": "happy.path.success",
                            "requests": [
                                {
                                    "environment": {
                                        "contents": "",
                                        "title": "Upload from before block"
                                    },
                                    "responseProcessing": {
                                        "variableAssignments": {
                                            "httpStatus": 200,
                                            "contentType": "application/json; charset=utf-8",
                                            "variables": {
                                                "before_picture_id": "${json_pointer(json(http.response.body),'/_id')}"
                                            }
                                        }
                                    },
                                    "$ref": "#/requests/uploadPicture"
                                },
                                {   
                                    "environment": {
                                        "picture_id": "{{before_picture_id}}"
                                    },
                                    "$ref": "#/requests/deletePicture"
                                }
                            ]
                        }
                    ]
                }
            },
            "/user/login": {
                "post": {
                    "happyPaths": [
                        {
                            "key": "happy.path.success",
                            "requests": [
                                {
                                    "$ref": "#/requests/login"
                                }
                            ]
                        }
                    ]
                }
            },
            "/user/register": {
                "post": {
                    "happyPaths": [
                        {
                            "key": "happy.path.success",
                            "requests": [
                                {
                                    "$ref": "#/requests/register"
                                },
                                {
                                    "$ref": "#/requests/deleteUser"
                                }
                            ]
                        }
                    ]
                }
            },
            "/admin/users/search": {
